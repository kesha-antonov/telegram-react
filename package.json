{
  "homepage": "http://evgeny-nadymov.github.io/telegram-react",
  "name": "telegram_react",
  "version": "0.0.340",
  "private": true,
  "engines": {
    "node": ">= 8.12.0",
    "yarn": "^1.13.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.2.0",
    "@material-ui/lab": "^4.0.0-alpha.16",
    "@material-ui/styles": "^4.1.1",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.0.8",
    "dateformat": "^3.0.3",
    "emoji-mart": "^2.10.0",
    "emoji-regex": "^8.0.0",
    "events": "^3.0.0",
    "husky": "^1.3.1",
    "i18next": "^15.0.7",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-localstorage-backend": "^2.1.2",
    "immutable": "^4.0.0-rc.12",
    "libphonenumber-js": "^1.7.13",
    "lint-staged": "^8.1.5",
    "localforage": "^1.7.3",
    "localforage-getitems": "^1.4.2",
    "lottie-web": "^5.5.4",
    "mdi-material-ui": "^6.0.0",
    "notistack": "^0.5.1",
    "prettier": "^1.16.4",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.6.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.0.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-devtools": "^3.5.0",
    "sw-precache-cra": "^1.0.0-alpha.2",
    "tdweb": "^1.4.3",
    "universal-cookie": "^3.1.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --print-width 120 --tab-width 4 --single-quote --jsx-single-quote --jsx-bracket-same-line --bracket-spacing --write --no-semi trailingComma:es5",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && sw-precache-cra --config sw-config.js --no-minify && mv build telegram-react && mkdir build && mv telegram-react build/",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "size": "react-scripts build --stats && npx webpack-bundle-analyzer ./build/bundle-stats.json",
    "sw-precache-cra-list-config": "sw-precache-cra --list-config"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
